    "def inspect_dataframe(input_df):\n",
    "    print('\\n','_'*50)\n",
    "    print(\"First 10 Rows of dataFrame\")\n",
    "    print(input_df.head(10))\n",
    "\n",
    "    print('\\n','_'*50)\n",
    "    print(\"Display of Null Values in dataFrame\")\n",
    "    print(input_df.isnull().sum())\n",
    "\n",
    "    print('\\n','_'*50)\n",
    "    print(\"Display of Dupicated values in dataFrame\")\n",
    "    print(input_df.duplicated().sum())\n",
    "\n",
    "    print('\\n','_'*50)\n",
    "    print(\"General info about DataFrame\")\n",
    "    print(input_df.info())\n",
    "\n",
    "    print('\\n','_'*50)\n",
    "    print(\"General info about DataFrame\")\n",
    "    print(input_df.info())\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "    return \"That gooood stuff.\"\n",
    "inspect_dataframe(df)\n",
